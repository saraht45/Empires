    <Core:PCProperties>
      <Core:Property property="command">"+emit"</Core:Property>
      <Core:Property property="description">
         "Emit a string into your environment. The '+'-character is used to substitute person names. Note that the +\<name\>-feature only works on characters without titles."
      </Core:Property>
      <Core:Property property="examples">
         "+emit A pink panda snarls at +Kalle deviously.\\r\\n     Emit the message into the user's environment. Everyone except Kalle receives \\"A pink panda snarls at Kalle deviously.\\" Kalle receives \\"A pink panda snarls at you deviously.\\""
      </Core:Property>
      <Core:Property property="last_mod">1095414235</Core:Property>
