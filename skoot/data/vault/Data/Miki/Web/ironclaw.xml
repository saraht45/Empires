      </Core:Property>
      <Core:Property property="merry:lib:listvpnotes">
         X[M] /* lists the notes about this VP */

if(\$char==nil) \{
  return "No character was selected.";
\}

\$object = "Chatters:IC:"+\$char[0..1]+":"+\$char;
\$object = Obj(\$object);

if(!\$object) \{
  return "Invalid character.";
\}

\$output = "\<h2\>"+Describe(\$object)+"\</h2\>";
\$output += "\<h3\>Character type: ";
if(\$object.ipc) \{
  \$output += "IPC - Influence Player Character";
\} else if(\$object.npc) \{
  \$output += "NPC - Non-Player Character";
\} else \{
  \$output += "PC - Player Character";
\}
\$output += "\</h3\>";

\$output += "\<h3\>Character Roles\</h3\>";
if(!\$object."vp:roles") \{
  \$output += "No roles found for this character.\<br/\>";
\} else \{
  for(\$i=0;\$i\<sizeof(\$object."vp:roles");\$i++) \{
    \$output += "Role "+Str(\$i+1)+": "+\$object."vp:roles"[\$i]+"\<br/\>";
  \}
\}

\$output += "\<h3\>Character Notes\</h3\>";
if(!\$object."vp:notes") \{
  \$output += "No notes found for this character.\<br/\>";
\} else \{
  /* list the notes */
  for(\$i=0;\$i\<sizeof(\$object."vp:notes");\$i++) \{
    if(Str(\$object."vp:notes"[\$i][1]) == "note") \{
      \$output += "\<font color=\\"red\\"\>note - ";
    \} else if(Str(\$object."vp:notes"[\$i][1]) == "warning") \{
      \$output += "\<font color=\\"red\\"\>warning - ";
    \} else if(typeof(\$object."vp:notes"[\$i][1]) == 3) \{
      \$output += "\<font\>"+\$object."vp:notes"[\$i][1]+" - ";
    \} else if(\$object."vp:notes"[\$i][1] \< 10) \{
      \$output += "\<font\>role "+Str(\$object."vp:notes"[\$i][1])+" ";
    \} else \{
      \$output +="\<font\>role "+Str(\$object."vp:notes"[\$i][1]);
    \}

    /* the timestamp */
    \$output += short_time(\$object."vp:notes"[\$i][0]);

    /* the actual note */
    \$output += " - "+\$object."vp:notes"[\$i][2];

    /* newline */
    \$output += "\</font\>\<br/\>";
  \}
\}

return \$output;
